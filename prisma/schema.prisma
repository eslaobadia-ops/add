model User {
  id       Int      @id @default(autoincrement())
  username String   @unique
  password String
  role     String
  paid     Boolean  @default(false)
  results  Result[]
}

model Result {
  id        Int    @id @default(autoincrement())
  subject   String
  score     Int
  grade     String
  userId    Int
  user      User   @relation(fields: [userId], references: [id])
}
